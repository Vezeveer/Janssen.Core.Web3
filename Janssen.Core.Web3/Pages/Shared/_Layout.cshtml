                </nav>
            </header>
        <div class="container sect-container">
            <div class="sect-content-container">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>

        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2020 - SAJCTI Saint Arnold Janssen College of Tacloban, Inc. - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>*@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    @*<script src="~/js/site.js" asp-append-version="true"></script>*@

    <!-- Plugin JavaScript -->
    <script src="~/lib/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for this template -->
    <!--<script src="@grayscaleJsVersion"></script>
    <script src="@siteJsVersion"></script>-->
    <script src="~/js/grayscale.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script>
        $(window).on("load", function () {
            window.location.href = "#"
            $(".loader-wrapper").fadeOut("slow");
            enableScrolling();
        });
    </script>
    <noscript>
        <style>
            .nojavascript {
                display: block;
            }
        </style>
    </noscript>

    @RenderSection("Scripts", required: false)
</body>
</html>





